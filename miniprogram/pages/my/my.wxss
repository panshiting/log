/* pages/my/my.wxss */
.head {
  width: 100%;
  height: 150px;
  background-color: #0e948a;
}
.user {
  vertical-align: center;
  height: 50px;
  padding: 30px 0 0 20px;
  display: flex;
}
.icon {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.username {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 10px;
  color: #ffffff;
  font-size: 14px;
}
.content {
  width: 100%;
  margin-top: 15px;
}
.content-item {
  width: 100%;
  font-size: 11pt;
  padding: 8px 15px;
  background-color: #ffffff;
  border-bottom: 1px solid #eeeeee;
  color: #333333;
}